{
    "family": "level-up-fargate", 
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::210874144112:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "ui-gaming-card-app", 
            "image": "ui-gaming-cards:1.0.0", 
            "portMappings": [
                {
                    "containerPort": 3001, 
                    "hostPort": 3001, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}
